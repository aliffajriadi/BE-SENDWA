datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  nomor String @unique
  name String
  token Int
  saldo Int @default(0)
  free_event Boolean @default(true)
  @@map("users")
}


model ApiKey {
  id String @unique
  key String
  owner String
  token Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  @@map("api_keys")
}
